<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SientoCon">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/mesientocontxt"
                    android:layout_width="375dp"
                    android:layout_height="38dp"
                    android:layout_gravity="center_horizontal|center"
                    android:layout_marginLeft="-3dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="sans-serif-black"
                    android:gravity="center"
                    android:text="Me siento con ________"
                    android:textColor="@color/black"
                    android:textSize="20sp" />


                <ImageView
                    android:id="@+id/mesientoconimg"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="mesientocon"
                    android:foreground="?android:attr/selectableItemBackground"
                    android:src="@drawable/yo_me_siento_con" />

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TableLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center">

                                <TextView
                                    android:id="@+id/gripe"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="sans-serif-black"
                                    android:gravity="center"
                                    android:text="Gripe"
                                    android:textColor="@color/black"
                                    android:textSize="20sp" />

                                <TextView
                                    android:id="@+id/dolordegarganta"
                                    android:layout_width="116dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="sans-serif-black"
                                    android:gravity="center"
                                    android:text="Dolor de garganta"
                                    android:textColor="@color/black"
                                    android:textSize="20sp" />

                                <TextView
                                    android:id="@+id/dolordespalda"
                                    android:layout_width="113dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="sans-serif-black"
                                    android:gravity="center"
                                    android:text="Dolor de espalda"
                                    android:textColor="@color/black"
                                    android:textSize="20sp" />
                            </TableRow>

                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center">

                                <Button
                                    android:id="@+id/gripeid"
                                    android:layout_width="117dp"
                                    android:layout_height="100dp"
                                    android:layout_marginStart="15dp"
                                    android:layout_marginTop="10dp"
                                    android:background="@drawable/gripe"
                                    android:contentDescription="gripe"
                                    android:foreground="?android:attr/selectableItemBackground"
                                    android:gravity="center"
                                    app:backgroundTint="@null" />

                                <Button
                                    android:id="@+id/gargantaid"
                                    android:layout_width="117dp"
                                    android:layout_height="100dp"
                                    android:layout_marginStart="15dp"
                                    android:layout_marginTop="10dp"
                                    android:background="@drawable/dolordegarganta"
                                    android:contentDescription="garganta"
                                    android:foreground="?android:attr/selectableItemBackground"
                                    android:gravity="center"
                                    app:backgroundTint="@null" />

                                <Button
                                    android:id="@+id/espaldaid"
                                    android:layout_width="117dp"
                                    android:layout_height="100dp"
                                    android:layout_marginStart="15dp"
                                    android:layout_marginTop="10dp"
                                    android:background="@drawable/dolordespalda"
                                    android:contentDescription="espalda"
                                    android:foreground="?android:attr/selectableItemBackground"
                                    android:gravity="center"
                                    app:backgroundTint="@null" />

                            </TableRow>

                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center">

                                <TextView
                                    android:id="@+id/dolordelcorazon"
                                    android:layout_width="87dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="sans-serif-black"
                                    android:gravity="center"
                                    android:text="Dolor en el corazón"
                                    android:textColor="@color/black"
                                    android:textSize="20sp" />

                                <TextView
                                    android:id="@+id/dolordestomago"
                                    android:layout_width="107dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="sans-serif-black"
                                    android:gravity="center"
                                    android:text="Dolor de Estomago"
                                    android:textColor="@color/black"
                                    android:textSize="20sp" />

                                <TextView
                                    android:id="@+id/dolordecapeza"
                                    android:layout_width="100dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="sans-serif-black"
                                    android:gravity="center"
                                    android:text="Dolor de Cabeza"
                                    android:textColor="@color/black"
                                    android:textSize="20sp" />
                            </TableRow>

                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="205dp"
                                android:gravity="center">

                                <Button
                                    android:id="@+id/corazon"
                                    android:layout_width="117dp"
                                    android:layout_height="100dp"
                                    android:layout_marginStart="15dp"
                                    android:layout_marginTop="10dp"
                                    android:background="@drawable/dolordecorazon"
                                    android:contentDescription="corazon"
                                    android:foreground="?android:attr/selectableItemBackground"
                                    android:gravity="center"
                                    app:backgroundTint="@null" />

                                <Button
                                    android:id="@+id/estomago"
                                    android:layout_width="117dp"
                                    android:layout_height="100dp"
                                    android:layout_marginStart="15dp"
                                    android:layout_marginTop="10dp"
                                    android:background="@drawable/dolordestomago"
                                    android:contentDescription="estomago"
                                    android:foreground="?android:attr/selectableItemBackground"
                                    android:gravity="center"
                                    app:backgroundTint="@null" />

                                <Button
                                    android:id="@+id/cabeza"
                                    android:layout_width="117dp"
                                    android:layout_height="100dp"
                                    android:layout_marginStart="15dp"
                                    android:layout_marginTop="10dp"
                                    android:background="@drawable/dolordecabeza"
                                    android:contentDescription="cabeza"
                                    android:foreground="?android:attr/selectableItemBackground"
                                    android:gravity="center"
                                    app:backgroundTint="@null" />
                            </TableRow>

                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center">

                                <TextView
                                    android:id="@+id/dolordemuela"
                                    android:layout_width="111dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="sans-serif-black"
                                    android:gravity="center"
                                    android:text="Dolor de muela"
                                    android:textColor="@color/black"
                                    android:textSize="20sp" />

                                <TextView
                                    android:id="@+id/dolordeoido"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="15dp"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="sans-serif-black"
                                    android:gravity="center"
                                    android:text="Dolor de oído"
                                    android:textColor="@color/black"
                                    android:textSize="20sp" />

                            </TableRow>

                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="center">

                                <Button
                                    android:id="@+id/muela"
                                    android:layout_width="117dp"
                                    android:layout_height="100dp"
                                    android:layout_marginStart="15dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginBottom="15dp"
                                    android:background="@drawable/dolordemuela"
                                    android:contentDescription="muela"
                                    android:foreground="?android:attr/selectableItemBackground"
                                    android:gravity="center"
                                    app:backgroundTint="@null" />

                                <Button
                                    android:id="@+id/oido"
                                    android:layout_width="117dp"
                                    android:layout_height="100dp"
                                    android:layout_marginStart="15dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginBottom="15dp"
                                    android:background="@drawable/doloroido"
                                    android:contentDescription="oido"
                                    android:foreground="?android:attr/selectableItemBackground"
                                    android:gravity="center"
                                    app:backgroundTint="@null" />
                            </TableRow>

                        </TableLayout>
                    </LinearLayout>


                </ScrollView>

            </LinearLayout>

        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</RelativeLayout>